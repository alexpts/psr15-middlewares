{
    "name": "alexpts/psr15-middlewares",
    "description": "Middlewares compatible with PSR-7 and PSR-15",
    "homepage": "https://github.com/alexpts/psr15-middlewares",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "middleware",
        "psr-7",
        "psr-15"
    ],

    "authors": [{
        "name": "Alexpts",
        "email": "alexpts3@gmail.com"
    }],
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "require": {
        "php": "~7.2",
        "ext-json": "*",
        "psr/http-server-middleware": "^1.0",
        "psr/http-message": "^1.0",
        "laminas/laminas-diactoros": "^2.2",
        "psr/log": "^1.0",
        "laminas/laminas-httphandlerrunner": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "PTS\\PSR15\\Middlewares\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    }
}
